{
  "name": "kitzberger/usebouncer",
  "type": "typo3-cms-extension",
  "description": "Mail address validation via usebouncer.com (for powermail)",
  "homepage": "https://github.com/kitzberger/usebouncer",
  "authors": [
    {
      "name": "Philipp Kitzberger",
      "role": "Developer"
    }
  ],
  "license": [
    "GPL-2.0-or-later"
  ],
  "keywords": [
    "TYPO3 CMS",
    "powermail",
    "usebouncer.com"
  ],
  "require": {
    "typo3/cms-core": "^11.5 || ^12.4"
  },
  "suggest": {
    "in2code/powermail": "*"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "usebouncer"
    }
  },
  "autoload": {
    "psr-4": {
      "Kitzberger\\Usebouncer\\": "Classes"
    }
  }
}
